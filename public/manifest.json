{
  "name": "Dice Roller",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "A simple Dice Roller extension",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "scripting"
  ],
  "icons": {
    "48": "./dice.png",
    "96": "./dice.png"
  }
}
